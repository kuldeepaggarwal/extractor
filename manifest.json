{
  "name": "Extractor",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Extractor",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "Extractor",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "scripts/contentsripts.js"
      ],
      "css": [
        "stylesheets/contentsripts.css"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "history",
    "management",
    "http://*/*",
    "https://*/*",
    "contextMenus"
  ]
}
